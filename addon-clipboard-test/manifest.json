{

  "manifest_version": 2,
  "name": "Clipboard-Test",
  "version": "1.1",

  "description": "Reads the content of the clipboard and writes it to the console.",
  "icons": {
    "48": "icons/clipboard-test-48.png"
  },

  "permissions": [
    "clipboardRead"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["background.js"]
    }
  ]

}


